{
	"name": "dmzx/workflowbadge",
	"type": "phpbb-extension",
	"description": "This Extension will add a new BBcode for Github workflow tests.",
	"homepage": "https://www.dmzx-web.net",
	"version": "1.0.0",
	"time": "2021-07-19",
	"keywords": [
		"phpbb",
		"extension",
		"workflowbadge"
	],
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "dmzx",
			"homepage": "https://www.dmzx-web.net",
			"email": "info@dmzx-web.net",
			"role": "Extension Developer"
		}
	],
	"require": {
		"php": ">=7.1.3",
		"composer/installers": "~1.0"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
	"extra": {
		"display-name": "Workflow badge",
		"soft-require": {
			"phpbb/phpbb": ">=3.3.0,<4.0.0@dev"
		},
		"version-check": {
			"host": "www.dmzx-web.net",
			"directory": "/versions",
			"filename": "workflowbadge_version.json",
			"ssl": true
		}
	}
}
